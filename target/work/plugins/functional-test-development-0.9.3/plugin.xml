<plugin name='functional-test-development' version='0.9.3' grailsVersion='* &gt; 2.0.0'>
  <author>Luke Daley</author>
  <authorEmail>ld@ldaley.com</authorEmail>
  <title>Functional Test Development</title>
  <description>Provides a script to make developing functional tests quicker</description>
  <documentation>http://grails.org/plugin/functional-test-development</documentation>
  <type>FunctionalTestDevelopmentGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.codehaus.geb' name='geb-junit4' version='0.7.2' />
      <dependency group='org.seleniumhq.selenium' name='selenium-chrome-driver' version='2.25.0' />
    </test>
  </dependencies>
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='hibernate' version='2.1.1' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.1.1' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>