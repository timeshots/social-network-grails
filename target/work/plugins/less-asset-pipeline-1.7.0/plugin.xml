<plugin name='less-asset-pipeline' version='1.7.0' grailsVersion='2.0 &gt; *'>
  <author>David Estes</author>
  <authorEmail>destes@bcap.com</authorEmail>
  <title>LESS Asset-Pipeline Plugin</title>
  <description>Provides LESS support for the asset-pipeline static asset management plugin.</description>
  <documentation>http://github.com/bertramdev/less-grails-asset-pipeline</documentation>
  <type>LessAssetPipelineGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='com.github.sommeri' name='less4j' version='1.3.0' />
    </compile>
  </dependencies>
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='asset-pipeline' version='1.7.0' />
    </runtime>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>